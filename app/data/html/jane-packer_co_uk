nt['ThumbStudio_Image'].src = ThumbStudio_getimg(ThumbStudio_imgarray[ThumbStudio_pic]);      for(i=0;i<=100;i+=5) {setTimeout(\"ThumbStudio_trans(\"+i+\")\",(i*2));}ThumbStudio_changetext('ThumbStudio_Counter', (ThumbStudio_pic+1) + '/' + ThumbStudio_nopic);      ThumbStudio_changetext('ThumbStudio_Comment', ThumbStudio_comarray[ThumbStudio_pic]);      }//-->    </script>  <style type=\"text/css\">      .ThumbStudioCSS {font-family:Verdana; font-size:11px; color:#000000; }  .ThumbStudioNAV a {text-decoration:none; font-family:Verdana; font-size:11px; color:#000000; }  .ThumbStudioNAV a:link {text-decoration:none; font-family:Verdana; font-size:11px; color:#000000; }  .ThumbStudioNAV a:visited {text-decoration:none; font-family:Verdana; font-size:11px; color:#000000; }  .ThumbStudioNAV a:active {text-decoration:none; font-family:Verdana; font-size:11px; color:#000000; }  .ThumbStudioNAV a:hover {text-decoration:none; font-family:Verdana; font-size:11px; color:#000000; }  .ThumbStudioNAV {text-decoration:none; font-family:Verdana; font-size:11px; color:#000000; }  .style13 {   text-align: left;   color: #FFFFFF;  }  .style14 {   border-style: none;   font-family: Verdana;   font-size: 11px;   color: #000000;  }  .auto-style8 {   border-width: 0;   font-family: Verdana;   font-size: 11px;   color: #000000;  }  .LeftBlockTextBoxStyle {   width: 400px;   height: 400px;   top: 0px;   right: 0px;   bottom: 0px;   left: 0px;   margin: 25px;   font-family: \"Lucida Sans\", \"Lucida Sans Regular\", \"Lucida Grande\", \"Lucida Sans Unicode\", Geneva, Verdana, sans-serif;   color: #C0C0C0;   font-size: small;   font-weight: normal;   font-style: normal;   font-variant: normal;   text-transform: none;  }  .style15 {   font-family: \"Arial Narrow\";   font-size: small;  }  .BoldWhiteTextStyle {   color: #FFFFFF;  }  </style>  <table class=\"auto-style8\" cellspacing=\"0\" cellpadding=\"0\" bgcolor=\"#000000\" align=\"right\" style=\"width: 450px; height: 418px;\">  <tr>  <td wid